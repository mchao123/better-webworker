{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ES6",
    "lib": [
      "DOM",
      "ES2018"
    ],
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "allowJs": true,
  },
  "include": [
    "*.ts",
    "*.mts",
  ],
  "exclude":[]
}
